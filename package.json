{
  "dependencies": {
    "mobx-state-tree": "^5.2.0",
    "puppeteer": "^21.3.7"
  },
  "devDependencies": {
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build": "rm -rf ./build/node/* && webpack --config webpack.config.js",
    "test:node": "rm -f ./results/node.txt && npm run build && ./benchmark-node.sh &> ./results/node.txt",
    "test:hermes": "rm -f ./results/hermes.txt && npm run build && ./benchmark-hermes.sh &> ./results/hermes.txt",
    "test:web": "rm -f ./results/web.txt && npm run build && node browser-tests.js &> ./results/web.txt",
    "test": "npm run test:node && npm run test:hermes && npm run test:web"
  }
}
